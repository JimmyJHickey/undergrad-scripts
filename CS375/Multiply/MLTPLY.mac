;23456789012345678901234567890123
START:	MOV	ZERO,OUT	;SET PROD TO 0
S2:	MOV	IN1,CNTR	;SET COUNTER TO IN1
S3:	BNE	S4		;CHECK IF COUNTER=0
	BR 	DONE		;BRANCH IF COUNTER IS 0
S4:	ADD	IN2,OUT		;ADD IN2 TO THE PRODUCT
	SUB	ONE,CNTR	;DECREMENT COUNTER
	BR	S3		;GO BACK TO S3
DONE:	HALT
IN1:	.BLKW	1		;IN-ZERO OR POS 2'S COMPL
IN2:	.BLKW	1		;IN-ZERO OR POS 2'S COMPL
OUT:	.BLKW	1		;OUT-PRODUCT, 2'S COMPL
CNTR:	.BLKW	1		;COUNTER
ZERO:	.WORD	0		;MAKE CONSTANT 0
ONE:	.WORD	1		;MAKE CONSTANT 1